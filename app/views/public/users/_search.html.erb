<% users.each do |user| %>
  <table>
    <tr>
     <td><%= link_to public_user_path(user) do %>
      <%= attachment_image_tag(user, :profile_image, :fill, 300, 300, fallback: "no_image.jpg", class:"icon") %>
      <%= user.name %><br></td>
  <% end %>
  <%if user.introduction!=nil%>
    <td class="intro"><%= user.introduction %></td>
  <% end %>
    </tr>
  </table>
<% end %>