<h1>お問い合わせ一覧</h1>
  <table class="table">
   <thead>
    <tr>
      <th scope="col">名前</th>
      <th scope="col">内容</th>
      <th scope="col">Email</th>
      <th scope="col">受信日</th>
      <th scope="col">対応ステータス</th>
    </tr>
  </thead>
   <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.body %></td>
        <td><%= contact.email %></td>
        <td><%= contact.created_at%></td>
            <%if contact.is_deleted == false%>
              <td>未</td>
            <% else %>
              <td>済</td>
            <%end%>
        <td><%= link_to "対応完了", contact_path(contact), method: :PATCH, class: "glyphicon glyphicon-tint btn btn-danger" %></td>
      </tr>
    <% end %>
</table>