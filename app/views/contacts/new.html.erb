  <%= form_with model: @contact, local:true do |f| %>
    <table>
      <tr>
        <td><%= f.label :"お名前"%></td>
         <td><%= f.text_field :name %></td>
      </tr>
      <tr>
         <td><%= f.label :"メールアドレス"%></td>
         <td><%= f.email_field :email %></td>
      </tr>
      <tr>
         <td><%= f.label :"お問合せ内容"%></td>
         <td><%= f.text_area :body %></td>
      </tr>
    </table>
      <%= f.submit "送信"%>
  <% end %>